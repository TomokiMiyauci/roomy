    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "nuxt": "^2.11.0",
    "nuxt-i18n": "^6.6.0",
    "vuex-module-decorators": "^0.16.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "firebase-functions-test": "^0.1.6",
    "typescript": "^3.2.2"
